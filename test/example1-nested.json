{
  "loadcollection1": {
    "process_id": "load_collection",
    "arguments": {
      "bands": ["B10"],
      "id": "COPERNICUS/S2",
      "spatial_extent": {
        "west": 16.06,
        "south": 48.06,
        "east": 16.65,
        "north": 48.35
      },
      "temporal_extent": ["2020-01-20", "2020-01-30"]
    }
  },
  "saveresult1": {
    "process_id": "save_result",
    "arguments": {
      "data": {
        "process_id": "reduce_dimension",
        "arguments": {
          "data": { "from_node": "loadcollection1" },
          "dimension": "t",
          "reducer": {
            "process_graph": {
              "min1": {
                "process_id": "min",
                "arguments": { "data": { "from_parameter": "data" } },
                "result": true
              }
            }
          }
        }
      },
      "format": "JPEG",
      "options": {}
    },
    "result": true
  }
}
